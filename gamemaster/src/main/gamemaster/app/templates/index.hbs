{{#link-to 'plot-point'}}New Plot Point{{/link-to}}	
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th/>
		</tr>
	</thead>
	<tbody>
		{{#each plot-point in model}}
			<tr>
				<td>{{plot-point.name}}</td>
				<td>{{{plot-point.description}}}</td>
				<td>
					<button class="btn btn-default btn-xs"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button>
					<button class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span></button>
				</td>
			</tr>
		{{/each}}
	</tbody>
</table>
